{
    "name": "nyeholt/silverstripe-extensible-elastic",
    "description": "An elastic search adapter for extensible search",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "search",
        "elastic search",
        "full-text"
    ],
    "homepage": "https://github.com/nyeholt/silverstripe-extensible-elastic",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Marcus Nyeholt",
        "email": "marcus@symbiote.com.au"
    }],
    "require": {
        "silverstripe/cms": "^5",
        "nglasl/silverstripe-extensible-search": "*",
        "heyday/silverstripe-elastica": "*",
        "symbiote/silverstripe-multivaluefield": "*"
    },
    "require-dev": {
            "cambis/silverstripe-rector": "^0.7",
            "phpunit/phpunit": "^9.5",
            "syntro/silverstripe-phpstan": "^5",
            "nswdpc/ci-files": "^1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nswdpc/ci-files.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/JamesDPC/silverstripe-extensible-search.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symbiote\\ElasticSearch\\": "src/",
            "Elastica\\Transport\\": "src/transport"
        }
    },
    "extra": {
        "installer-name": "extensible-elastic",
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
