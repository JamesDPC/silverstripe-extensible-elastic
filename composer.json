	"keywords": [
		"silverstripe",
		"search",
		"elastic search",
		"full-text"
	],
	"homepage": "https://github.com/nyeholt/silverstripe-extensible-elastic",
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Marcus Nyeholt",
		"email": "marcus@symbiote.com.au"
	}],
	"require": {
		"silverstripe/cms": "~4.0",
